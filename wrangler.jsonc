/**
 * AQI Notify - Cloudflare Worker Configuration
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "aqi-notify",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-27",
	"observability": {
		"enabled": true
	},
	"triggers": {
		"crons": [
			"0 * * * *",
			"0 1 * * *"
		]
	}
}
